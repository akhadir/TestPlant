{
  "manifest_version": 2,
  "name": "Testcase Plant",
  "description": "Testcase Fruits Generated Here",
  "version": "0.1",
  "minimum_chrome_version": "10.0",
  "browser_action": {
    "default_icon": "icon.png",
    //"default_popup": "popup.html",
    "default_title": "Generate Testcases here!"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": [],
      "js": ["extJs/jquery-1.8.3.js", "inject.js"],
      "run_at": "document_start"
    }
  ],
  "devtools_page": "devtools.html",
  "background": {
    "scripts": ["js/bgNew.js"]
  },

  "permissions": [
    "debugger",
    "activeTab",
    "background",
    "tabs",
    "https://*/",
    "https://*/*",
    "http://*/",
    "http://*/*"
  ]
}